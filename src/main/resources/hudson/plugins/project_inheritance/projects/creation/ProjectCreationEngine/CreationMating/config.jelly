<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core" xmlns:f="/lib/form" xmlns:pf="/form">
   	
	<f:entry title="${%First Class}" field="firstClass">
		<f:select/>
	</f:entry>
	
	<f:entry title="${%Second Class}" field="secondClass">
		<f:select/>
	</f:entry>
	
	<f:entry title="${%Description}" field="description">
		<f:expandableTextbox/>
	</f:entry>
	
	<f:validateButton
		title="${%Check number of generated Jobs}" progress="${%Computing...}"
		method="checkNumberOfMates" with="firstClass,secondClass"
	/>
	
</j:jelly>